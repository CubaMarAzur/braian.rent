{
  "name": "braian.rent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:standalone": "node .next/standalone/server.js",
    "start:prod": "NODE_ENV=production node .next/standalone/server.js",
    "lint": "eslint",
    "db:seed": "npx prisma db seed",
    "db:migrate": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "validate:env": "node -e \"require('./src/lib/env.ts')\"",
    "docker:build": "docker build -t braian-rent .",
    "docker:run": "docker run -p 3000:8080 --env-file .env braian-rent"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-prometheus": "^0.206.0",
    "@opentelemetry/instrumentation-express": "^0.55.0",
    "@opentelemetry/instrumentation-http": "^0.206.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@prisma/client": "^6.17.0",
    "@sentry/nextjs": "^10.18.0",
    "autoprefixer": "^10.4.21",
    "next": "15.5.4",
    "next-auth": "^5.0.0-beta.29",
    "pino": "^10.0.0",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "winston": "^3.18.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "bcryptjs": "^3.0.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "prisma": "^6.17.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
